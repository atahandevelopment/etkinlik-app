{
  "expo": {
    "name": "Aktivport",
    "slug": "aktivport",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/aktivport.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/aktivport.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF2D2D"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/aktivport.png",
        "backgroundColor": "#FF2D2D"
      },
      "package": "com.atahandevelopment.aktivport"
    },
    "web": {
      "favicon": "./assets/aktivport.png"
    },
    "extra": {
      "eas": {
        "projectId": "1670593d-d795-4473-9062-407e0f360c4c"
      }
    },
    "plugins": [
      [
          "expo-build-properties", {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Aktivport isimli uygulama kamera erişimi için izin istiyor."
        }
      ]
    ]
  }
}
